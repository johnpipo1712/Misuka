@using Misuka.Infrastructure.MVC
@{
    ViewBag.Title = this.LocalResources("PageTitle");
    Layout = "~/Views/Shared/Admin/_Layout.cshtml";
}

  <div class="row margin-top-10 clearfix no-border">
      <div class=" col-xs-12">
        <button class="btn btn-primary btn-sm pull-left" onclick="WebSiteLinkClient.OnAddNew()"><i class="fa fa-plus"></i>@Misuka.Web.Resources.Common.Add</button>
        <button class="btn btn-primary btn-sm pull-left" onclick="WebSiteLinkClient.OnDelete('@Misuka.Web.Resources.Common.DeleteComfirmMessage')"><i class="fa fa-remove"></i>@Misuka.Web.Resources.Common.Delete</button>
      </div>
    </div>
        <div class="row">
            <div class="col-xs-12 margin-top-10">
              <table id="WebSiteLinkGrid"></table>
                <div id="pager"></div>
            </div>
        </div>

  
@section Scripts
{
<script src="@Url.Content("~/Scripts/pages/WebSiteLink/websitelink.js")" type="text/javascript"></script>
    <script type="text/javascript">
        var colNames = ['WebSiteLinkId', '', '@Misuka.Web.Resources.WebSiteLink.Name', '@Misuka.Web.Resources.WebSiteLink.Link',  ' '];
        $(document).ready(function () {
            WebSiteLinkClient.Init();
        });

    </script>
}

